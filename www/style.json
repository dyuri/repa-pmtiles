{
  "version": 8,
  "id": "hungarian-hiking-dark",
  "name": "Hungarian Hiking - Dark",
  "glyphs": "http://localhost:8080/fonts/{fontstack}/{range}.pbf",
  "sources": {
    "hungary-hiking": {
      "type": "vector",
      "url": "pmtiles://http://localhost:8080/tiles/hungary-hiking.pmtiles",
      "attribution": "Â© OpenStreetMap contributors"
    },
    "contours": {
      "type": "vector",
      "url": "pmtiles://http://localhost:8080/tiles/hungary-contours.pmtiles",
      "attribution": "SRTM"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "minzoom": 0,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "background-color": [
          "interpolate",
          ["exponential", 1],
          ["zoom"],
          5,
          "hsl(86, 30%, 22%)",
          14,
          "hsl(120, 4%, 22%)"
        ]
      }
    },
    {
      "id": "forest",
      "type": "fill",
      "source": "hungary-hiking",
      "source-layer": "landuse",
      "minzoom": 9,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": "hsl(91, 38%, 14%)",
        "fill-opacity": 1
      },
      "filter": ["==", "type", "forest"]
    },
    {
      "id": "grass",
      "type": "fill",
      "source": "hungary-hiking",
      "source-layer": "landuse",
      "minzoom": 11,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": "hsla(90, 35%, 9%, 0.3)"
      },
      "filter": ["in", "type", "grass", "meadow", "park"]
    },
    {
      "id": "farmland",
      "type": "fill",
      "source": "hungary-hiking",
      "source-layer": "landuse",
      "minzoom": 11,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": "hsla(58, 55%, 13%, 0.4)"
      },
      "filter": ["in", "type", "farmland", "orchard", "vineyard"]
    },
    {
      "id": "water",
      "type": "fill",
      "source": "hungary-hiking",
      "source-layer": "water",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": [
          "interpolate",
          ["linear"],
          ["zoom"],
          6,
          "hsl(210, 61%, 20%)",
          14,
          "hsl(210, 61%, 18%)"
        ]
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "hungary-hiking",
      "source-layer": "waterway",
      "minzoom": 8,
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "hsl(210, 61%, 22%)",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          8,
          0.5,
          12,
          1.5,
          14,
          2.5
        ]
      }
    },
    {
      "id": "boundaries",
      "type": "line",
      "source": "hungary-hiking",
      "source-layer": "boundaries",
      "minzoom": 3,
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "hsl(0, 0%, 32%)",
        "line-dasharray": [3, 2],
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          3,
          0.5,
          10,
          1.5
        ],
        "line-opacity": 0.6
      }
    },
    {
      "id": "contour",
      "type": "line",
      "source": "contours",
      "source-layer": "contour",
      "minzoom": 11,
      "layout": {
        "line-cap": "square",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "hsl(36, 24%, 43%)",
        "line-width": 0.5,
        "line-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          11,
          0.15,
          13,
          0.4,
          15,
          0.6
        ]
      },
      "filter": [
        "all",
        ["!=", ["get", "elevation"], 0],
        ["!=", ["%", ["get", "elevation"], 100], 0]
      ]
    },
    {
      "id": "contour-index",
      "type": "line",
      "source": "contours",
      "source-layer": "contour",
      "minzoom": 10,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "hsl(36, 24%, 35%)",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10,
          0.8,
          13,
          1.2,
          15,
          1.8
        ],
        "line-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10,
          0.3,
          13,
          0.6,
          15,
          0.8
        ]
      },
      "filter": [
        "all",
        ["!=", ["get", "elevation"], 0],
        ["==", ["%", ["get", "elevation"], 100], 0]
      ]
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "hungary-hiking",
      "source-layer": "buildings",
      "minzoom": 13,
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-antialias": true,
        "fill-color": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13,
          "hsl(58, 0%, 12%)",
          18,
          "hsl(58, 0%, 10%)"
        ],
        "fill-outline-color": "hsl(58, 0%, 11%)"
      }
    },
    {
      "id": "roads-casing",
      "type": "line",
      "source": "hungary-hiking",
      "source-layer": "roads",
      "minzoom": 5,
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "hsl(0, 0%, 18%)",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          5,
          [
            "match",
            ["get", "type"],
            "motorway",
            1.5,
            0
          ],
          9,
          [
            "match",
            ["get", "type"],
            ["motorway", "trunk"],
            3,
            ["primary", "secondary"],
            2,
            0.5
          ],
          12,
          [
            "match",
            ["get", "type"],
            "motorway",
            8,
            "trunk",
            7,
            "primary",
            6,
            "secondary",
            5,
            "tertiary",
            4,
            ["residential", "unclassified", "living_street"],
            3,
            2
          ],
          14,
          [
            "match",
            ["get", "type"],
            "motorway",
            10,
            "trunk",
            9,
            "primary",
            8,
            "secondary",
            7,
            "tertiary",
            6,
            ["residential", "unclassified", "living_street"],
            5,
            3
          ],
          16,
          [
            "match",
            ["get", "type"],
            "motorway",
            16,
            "trunk",
            14,
            "primary",
            12,
            "secondary",
            10,
            "tertiary",
            8,
            ["residential", "unclassified", "living_street"],
            6,
            4
          ]
        ]
      }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "hungary-hiking",
      "source-layer": "roads",
      "minzoom": 5,
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": [
          "match",
          ["get", "type"],
          "motorway",
          "hsl(26, 87%, 47%)",
          "trunk",
          "hsl(30, 84%, 53%)",
          "primary",
          "hsl(46, 77%, 56%)",
          "secondary",
          "hsl(60, 60%, 55%)",
          "tertiary",
          "hsl(0, 0%, 42%)",
          ["residential", "unclassified", "living_street"],
          "hsl(0, 0%, 38%)",
          ["service", "road", "minor"],
          "hsl(0, 0%, 32%)",
          "hsl(0, 0%, 35%)"
        ],
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          5,
          [
            "match",
            ["get", "type"],
            "motorway",
            0.5,
            0
          ],
          9,
          [
            "match",
            ["get", "type"],
            ["motorway", "trunk"],
            1.5,
            ["primary", "secondary"],
            0.8,
            0.3
          ],
          12,
          [
            "match",
            ["get", "type"],
            "motorway",
            4,
            "trunk",
            3.5,
            "primary",
            2.5,
            "secondary",
            2,
            "tertiary",
            1.5,
            ["residential", "unclassified", "living_street"],
            1,
            ["service", "road", "minor"],
            0.5,
            1
          ],
          14,
          [
            "match",
            ["get", "type"],
            "motorway",
            6,
            "trunk",
            5,
            "primary",
            4,
            "secondary",
            3.5,
            "tertiary",
            3,
            ["residential", "unclassified", "living_street"],
            2,
            ["service", "road", "minor"],
            1,
            1.5
          ],
          16,
          [
            "match",
            ["get", "type"],
            "motorway",
            12,
            "trunk",
            10,
            "primary",
            8,
            "secondary",
            7,
            "tertiary",
            6,
            ["residential", "unclassified", "living_street"],
            4,
            ["service", "road", "minor"],
            2,
            3
          ]
        ]
      }
    },
    {
      "id": "trails-outline",
      "type": "line",
      "source": "hungary-hiking",
      "source-layer": "trails",
      "minzoom": 10,
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-blur": 0.5,
        "line-color": "hsla(120, 4%, 28%, 0.75)",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10,
          3,
          14,
          5,
          16,
          6
        ],
        "line-opacity": 0.8
      }
    },
    {
      "id": "trails",
      "type": "line",
      "source": "hungary-hiking",
      "source-layer": "trails",
      "minzoom": 10,
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": [
          "case",
          ["has", "color"],
          ["get", "color"],
          "hsl(15, 85%, 55%)"
        ],
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10,
          1.5,
          14,
          3,
          16,
          4
        ]
      }
    },
    {
      "id": "pois",
      "type": "circle",
      "source": "hungary-hiking",
      "source-layer": "pois",
      "minzoom": 12,
      "paint": {
        "circle-radius": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12,
          [
            "match",
            ["get", "type"],
            "peak",
            4,
            "viewpoint",
            3,
            "alpine_hut",
            3,
            "shelter",
            3,
            2
          ],
          16,
          [
            "match",
            ["get", "type"],
            "peak",
            8,
            "viewpoint",
            6,
            "alpine_hut",
            6,
            "shelter",
            5,
            4
          ]
        ],
        "circle-color": [
          "match",
          ["get", "type"],
          "peak",
          "hsl(25, 65%, 50%)",
          "viewpoint",
          "hsl(355, 75%, 65%)",
          "alpine_hut",
          "hsl(180, 55%, 55%)",
          "shelter",
          "hsl(200, 60%, 60%)",
          "drinking_water",
          "hsl(210, 70%, 60%)",
          "hsl(0, 0%, 60%)"
        ],
        "circle-stroke-width": 2,
        "circle-stroke-color": "hsl(0, 0%, 12%)",
        "circle-opacity": 0.9
      }
    },
    {
      "id": "contour-labels",
      "type": "symbol",
      "source": "contours",
      "source-layer": "contour",
      "minzoom": 13,
      "layout": {
        "symbol-avoid-edges": true,
        "symbol-placement": "line",
        "text-allow-overlap": false,
        "text-field": "{elevation}m",
        "text-font": ["Noto Sans Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13,
          8,
          16,
          10
        ],
        "text-rotation-alignment": "map"
      },
      "paint": {
        "text-color": "hsl(38, 50%, 60%)",
        "text-halo-color": "hsl(120, 4%, 18%)",
        "text-halo-width": 1.5,
        "text-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13,
          0.6,
          15,
          0.9
        ]
      },
      "filter": ["==", ["%", ["get", "elevation"], 100], 0]
    },
    {
      "id": "road-labels",
      "type": "symbol",
      "source": "hungary-hiking",
      "source-layer": "roads",
      "filter": ["has", "name"],
      "minzoom": 12,
      "layout": {
        "text-field": [
          "coalesce",
          ["get", "name"],
          ["get", "ref"]
        ],
        "text-font": ["Noto Sans Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12,
          [
            "match",
            ["get", "type"],
            ["motorway", "trunk"],
            10,
            ["primary", "secondary"],
            9,
            8
          ],
          16,
          [
            "match",
            ["get", "type"],
            ["motorway", "trunk"],
            13,
            ["primary", "secondary"],
            12,
            11
          ]
        ],
        "symbol-placement": "line",
        "text-rotation-alignment": "map",
        "text-pitch-alignment": "viewport",
        "text-max-angle": 30
      },
      "paint": {
        "text-color": [
          "match",
          ["get", "type"],
          ["motorway", "trunk", "primary", "secondary"],
          "hsl(60, 80%, 75%)",
          "hsl(0, 0%, 65%)"
        ],
        "text-halo-color": "hsl(0, 0%, 10%)",
        "text-halo-width": 2,
        "text-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12,
          0.7,
          14,
          1
        ]
      }
    },
    {
      "id": "waterway-labels",
      "type": "symbol",
      "source": "hungary-hiking",
      "source-layer": "waterway",
      "filter": ["has", "name"],
      "minzoom": 13,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Regular"],
        "text-size": 10,
        "symbol-placement": "line",
        "text-rotation-alignment": "map"
      },
      "paint": {
        "text-color": "hsl(210, 60%, 55%)",
        "text-halo-color": "hsl(0, 0%, 10%)",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "poi-labels",
      "type": "symbol",
      "source": "hungary-hiking",
      "source-layer": "pois",
      "filter": ["has", "name"],
      "minzoom": 13,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13,
          9,
          16,
          11
        ],
        "text-anchor": "top",
        "text-offset": [0, 1]
      },
      "paint": {
        "text-color": "hsl(0, 0%, 85%)",
        "text-halo-color": "hsl(0, 0%, 10%)",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "trail-labels",
      "type": "symbol",
      "source": "hungary-hiking",
      "source-layer": "trails",
      "filter": ["has", "name"],
      "minzoom": 13,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Noto Sans Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13,
          10,
          16,
          12
        ],
        "symbol-placement": "line",
        "text-rotation-alignment": "map"
      },
      "paint": {
        "text-color": "hsl(15, 85%, 70%)",
        "text-halo-color": "hsl(0, 0%, 10%)",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-labels",
      "type": "symbol",
      "source": "hungary-hiking",
      "source-layer": "place_labels",
      "minzoom": 6,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": [
          "case",
          ["==", ["get", "type"], "city"],
          ["literal", ["Noto Sans Bold"]],
          ["literal", ["Noto Sans Regular"]]
        ],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          6,
          [
            "match",
            ["get", "type"],
            "city",
            12,
            10
          ],
          12,
          [
            "match",
            ["get", "type"],
            "city",
            18,
            "town",
            15,
            "village",
            12,
            10
          ],
          16,
          [
            "match",
            ["get", "type"],
            "city",
            22,
            "town",
            18,
            "village",
            14,
            12
          ]
        ]
      },
      "paint": {
        "text-color": "hsl(0, 0%, 90%)",
        "text-halo-color": "hsl(0, 0%, 10%)",
        "text-halo-width": 2.5,
        "text-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          6,
          0.8,
          10,
          1
        ]
      }
    }
  ]
}
